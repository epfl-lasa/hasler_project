<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
	
	<xacro:macro name="gazebo_joints" params="name">
		<gazebo reference="${name}">
			<implicitSpringDamper>0</implicitSpringDamper>
			<provideFeedback>1</provideFeedback>
			<stopCfm></stopCfm> <!-- Force Mixing Error -->
			<stopErp></stopErp> <!-- Error Reduction Parameter -->
			<fudgeFactor>0.5</fudgeFactor>
		</gazebo>
	</xacro:macro>	
	
</robot>