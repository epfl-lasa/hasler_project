<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="number1">  
<link
	name="world"/>
  <link
	name="target/base_footprint"/>		
  <link
    name="target/base_link">
    <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <mass
        value="1" /> <!-- Look for real measurement -->
      <inertia
        ixx="1"
        ixy="0"
        ixz="0"
        iyy="1"
        iyz="0"
        izz="1" />
    </inertial>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <sphere radius="0.25"/>
      </geometry>
    </collision>
      <visual name="visual">
		<origin
		xyz="0 0 0"
        rpy="0 0 -1.5707963267" />
        <geometry>
          <mesh
            filename="package://foot_control_xy/models/number1/meshes/number.dae"/>
        </geometry>
      </visual>
  </link>
  <joint name="base_joint" type="fixed">
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<parent link="target/base_footprint"/>
		<child link="target/base_link"/>
  </joint>
  <joint name="base_footprint_joint" type="floating">
		<origin xyz="0 0 0" rpy="0 0 0"/>
		<parent link="world"/>
		<child link="target/base_footprint"/>
  </joint>
</robot>

