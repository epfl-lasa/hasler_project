      <uri>model://h_lower_limb</uri>
      <name>plane</name>
      <pose>0 0 0 0 0 0</pose>
    </include--> 
    
    <include>
      <uri>model://plane</uri>
      <name>plane</name>
      <pose>0 0 0 0 0 0</pose>
    </include> 
    
    
