<?xml version="1.0" ?>
<launch>
  <group ns="interface">
	<arg name="subjectName" default="walid"/>
	<arg name="executionMode" default="c"/>
	<arg name="trackingMode" default="t"/>
	<arg name="fittingMethod" default="p"/>
	<arg name="useKinect" default="false"/>
	<node output = "screen" pkg="hp_preliminary_experiment" type="preliminaryExperiment" name="preliminaryExperiment" args="$(arg subjectName) -e $(arg executionMode) -t $(arg trackingMode) -f $(arg fittingMethod)"/>
  </group>

</launch>
